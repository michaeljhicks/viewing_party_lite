<h1>Discover Movies</h1>

<%= button_to 'Top Rated Movies', dashboard_movies_path, method: :get, params: {query: 'top40rated'}  %>
<%= form_with url: dashboard_movies_path, method: :get, local: true do |form| %>
  <%= form.label :query %>
  <%= form.text_field :query, required: true %>
  <%= form.submit 'Search by Title' %>
<% end %>
